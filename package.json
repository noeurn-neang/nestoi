{
  "name": "nestoi",
  "version": "1.0.6",
  "description": "nestoi is a powerful Node.js library that combines the simplicity of Express and the robustness of Swagger to help you set up your Node.js projects faster. With nestoi, you'll be development-ready in no time, allowing you to focus on building your application without getting bogged down in setup tasks.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf node_modules package-lock.json dist && npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "run-s clean build",
    "dev": "nodemon"
  },
  "engines": {
    "node": ">= 20.9 < 21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noeurn-neang/nestoi.git"
  },
  "keywords": [
    "nestoi",
    "node",
    "express",
    "swagger",
    "resful"
  ],
  "author": "Mr. Neang",
  "license": "ISC",
